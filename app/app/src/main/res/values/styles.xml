<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Widget.Material3.SearchBar.person" parent="Widget.Material3.SearchBar">
        <item name="android:textAppearance">@style/TextAppearance.Material3.SearchBar.MyTitle</item>
        <item name="android:backgroundTint">?attr/colorSecondary</item>
        <item name="tintNavigationIcon">false</item>
    </style>
    <style name="TextAppearance.Material3.SearchBar.MyTitle" parent="TextAppearance.Material3.SearchBar">
        <item name="android:textSize">24sp</item>
        <item name="android:textColor">?attr/colorOnSecondary</item>
        <item name="android:textColorHint">?attr/colorOnSecondary</item>
    </style>
    <style name="Widget.Material3.BottomNavigationView.NavBar" parent="Widget.Material3.BottomNavigationView">
        <item name="labelVisibilityMode">unlabeled</item>
        <item name="android:background">?attr/colorPrimaryVariant</item>
        <item name="materialThemeOverlay">@style/ThemeOverlay.App.BottomNavigationView</item>
        <item name="itemIconSize">43sp</item>
    </style>
    <style name="ThemeOverlay.App.BottomNavigationView" parent="">
        <item name="colorOnSecondaryContainer">@color/blue_500</item>
        <item name="colorOnSurfaceVariant">?attr/colorOnSecondary</item>
    </style>
</resources>